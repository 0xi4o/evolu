{
  "name": "@evolu/react-native",
  "version": "7.0.1",
  "description": "Evolu for React Native",
  "keywords": [
    "evolu",
    "react-native",
    "react-hooks"
  ],
  "author": "Daniel Steigerwald <daniel@steigerwald.cz>",
  "license": "MIT",
  "repository": "evoluhq/evolu",
  "bugs": {
    "url": "https://github.com/evoluhq/evolu/issues"
  },
  "homepage": "https://evolu.dev",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "react-native": {
    "./index.js": "./dist/index.js"
  },
  "files": [
    "dist/**",
    "src/**",
    "README.md"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "rm -rf dist && tsc",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rm -rf .turbo node_modules dist",
    "format": "prettier --write \"src/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "expo-updates": "^0.24.11"
  },
  "devDependencies": {
    "@evolu/common": "workspace:*",
    "@evolu/common-react": "workspace:*",
    "@evolu/tsconfig": "workspace:*",
    "eslint": "^8.57.0",
    "eslint-config-evolu": "workspace:*",
    "expo": "^50.0.7",
    "expo-sqlite": "~13.2.2",
    "react-native": "0.73.4",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "peerDependencies": {
    "@evolu/common-react": "^6.0.0",
    "expo": "^50.0.7",
    "expo-sqlite": "~13.2.2",
    "react-native": "0.73.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.16"
  },
  "sideEffects": []
}
